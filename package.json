{
  "scripts": {
    "build": "node ./node_modules/better-npm-run build",
    "lint": "eslint -c .eslintrc js",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "proxy": "node ./node_modules/better-npm-run proxy",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run proxy\""
  },
  "betterScripts": {
    "build": {
      "command": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "proxy": {
      "command": "node webpack/dev-proxy.js",
      "env": {
        "NODE_PATH": "./js",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./js"
      }
    }
  },
  "devDependencies": {
    "autoprefixer-loader": "^3.2.0",
    "babel-core": "6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-transform-async-to-module-method": "^6.8.0",
    "babel-plugin-transform-class-properties": "^6.11.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "better-npm-run": "0.0.11",
    "clean-webpack-plugin": "^0.1.10",
    "concurrently": "2.2.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.5.0",
    "eslint-config-airbnb": "11.1.0",
    "eslint-plugin-import": "^1.15.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.2.1",
    "express": "^4.14.0",
    "express-http-proxy": "^0.10.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "http-proxy": "^1.14.0",
    "json-loader": "0.5.4",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "react-a11y": "0.3.3",
    "react-hot-loader": "3.0.0-beta.3",
    "strip-loader": "^0.1.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.2",
    "webpack-dev-server": "1.15.2"
  },
  "dependencies": {
    "babel": "6.5.2",
    "babel-plugin-typecheck": "3.9.0",
    "babel-polyfill": "^6.13.0",
    "babel-runtime": "^6.11.6",
    "bluebird": "^3.4.6",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "moment": "^2.15.0",
    "oc-react-components": "^0.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-dropzone": "^3.6.0",
    "react-responsive": "1.1.4",
    "react-scrolla": "^0.3.0",
    "react-time": "^4.2.0"
  }
}
